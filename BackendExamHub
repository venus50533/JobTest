/****** Object:  StoredProcedure [dbo].[usp_AddLog]    Script Date: 2024/12/20 下午 06:54:15 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE   PROCEDURE [dbo].[usp_AddLog]
    @JsonData NVARCHAR(MAX)
AS
BEGIN
    INSERT INTO MyOffice_ACPD (ACPD_SID, ACPD_Cname, ACPD_Ename, ACPD_Status, ACPD_LoginID, ACPD_LoginPWD)
    SELECT 
        JSON_VALUE(@JsonData, '$.acpd_sid'),
        JSON_VALUE(@JsonData, '$.acpd_cname'),
        JSON_VALUE(@JsonData, '$.acpd_ename'),
        JSON_VALUE(@JsonData, '$.acpd_status'),
        JSON_VALUE(@JsonData, '$.acpd_loginID'),
        JSON_VALUE(@JsonData, '$.acpd_loginPWD');
END;

GO
